{
    "manifest_version": 2,
    "name": "GLS - Minified guided solution",
    "description": "Google search Tooltip project",
    "version": "2.0",
    "background": {
        "scripts": ["jquery-3.5.1.min.js"],
        "persistent": false
    },  
    "content_scripts": [
        {
          "matches": ["https://www.google.com/"],
          "js": ["jquery-3.5.1.min.js", "player.js"]
        }
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "options_page": "index.html"


}   